{
  "name": "vue3-ts-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 8888 --host 0.0.0.0 --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.tsx --ignore-path .gitignore --fix src",
    "format": "prettier . --write",
    "commit": "cz",
    "prepare": "husky install",
    "mock": "json-server mock/db.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/core": "^7.18.2",
    "@milkdown/core": "^6.1.3",
    "@milkdown/plugin-listener": "^6.1.3",
