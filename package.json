    "@milkdown/preset-commonmark": "^6.1.3",
    "@milkdown/prose": "^6.1.3",
    "@milkdown/theme-nord": "^6.1.3",
    "@milkdown/vue": "^6.1.3",
    "@vueuse/core": "^8.5.0",
    "axios": "^0.27.2",
    "element-plus": "2.1.7",
    "fabric": "^5.2.1",
    "lodash": "^4.17.21",
    "pinia": "^2.0.14",
    "vue": "^3.2.31",
    "vue-dompurify-html": "^3.0.0",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@faker-js/faker": "^7.1.0",
